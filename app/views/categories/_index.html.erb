<% (Category.all).each do |category| %>
  <tr>
    <td><%= category.name %></td>
    <td><%= link_to "Editar", edit_category_path(category), remote: true, class: "btn btn-warning" %></td>
    <td><%= link_to "Eliminar", category_delete_path(category), remote: true, class: "btn btn-danger" %></td>
  </tr>
<% end %>
