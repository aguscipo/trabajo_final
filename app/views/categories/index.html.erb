<div class="container">
  <h1>Categorías</h1>
  <div class="well">
    <%= link_to "Nueva Categoría", new_category_path, remote: true, class: "btn btn-primary" %>
  </div>
  <div class="new-category"></div>
  <%= will_paginate %>
  <table class="table table-bordered table-striped">
    <thead>
      <tr>
        <th>Nombre</th>
        <td>&nbsp;</td>
        <td>&nbsp;</td>
      </tr>
    </thead>
    <tbody class="category-index">
      <%= render "index" %>
    </tbody>
  </table>
</div>
<div id="category-modal" class="modal fade"></div>
